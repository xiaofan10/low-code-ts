{
  "name": "low-code-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "prepare": "husky",
    "lint": "npx eslint --ext .js,.jsx,.ts,.tsx --fix .",
    "cm": "git add -A && git-cz",
    "mock": "nodemon ./server/index.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "src/*.{js,jsx,ts,tsx}": "eslint --fix",
    "src/*": "prettier --write"
  },
  "dependencies": {
    "-": "^0.0.1",
    "@reduxjs/toolkit": "^2.2.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.96",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "D": "^1.0.0",
    "add": "^2.0.6",
    "ahooks": "^3.7.11",
    "antd": "^5.16.4",
    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.1",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "sass": "^1.75.0",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@craco/craco": "^7.1.0",
    "@types/mockjs": "^1.0.10",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "koa": "^2.15.3",
    "koa-router": "^12.0.1",
    "prettier": "^3.2.5",
    "nodemon": "^3.1.0",
    "mockjs": "^1.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
